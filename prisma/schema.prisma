generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        String   @id @default(uuid())
  name      String
  sku       String   @unique
  price     Float
  quantity  Int
  status    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}